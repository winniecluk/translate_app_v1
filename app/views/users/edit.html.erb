<%= @user.job %> Account

<%= form_for @user do |f| %>
  <div class="field">
    <%= f.label :first_name %>
    <%= f.text_field :f_name %>
  </div>
  <div class="field">
    <%= f.label :last_name %>
    <%= f.text_field :l_name %>
  </div>
  <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :phone_number %>
    <%= f.text_field :phone_number %>
  </div>
  <div class="field">
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>
  <div class="field">
    <%= f.label :zip_code %>
    <%= f.text_field :zip_code %>
  </div>
  <div class="field">
    <%= f.label :country %>
    <%= f.text_field :country %>
  </div>
  <div class="field">
  Say something about yourself!<br>
    <%= f.label :bio %>
    <%= f.text_field :bio %>
  </div>
  <div class="field">
    <%= f.label :web_link %>
    <%= f.text_field :web_link %>
  </div>
  <div class="actions"><%= f.submit %></div>
<% end %>
